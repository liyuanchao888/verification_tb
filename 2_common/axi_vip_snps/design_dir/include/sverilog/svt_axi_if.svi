/**
    @grouphdr Interfaces svt_axi_if Top Level interface for AXI VIP components within CHI VIP. This is a sub-interface of svt_chi_if.
 */

/**
 * @groupname svt_axi_if
 * <b>General description:</b><p>
 * AXI VIP provides the SystemVerilog interface which can be used to connect
 * the VIP to the DUT. A top level interface svt_axi_if is defined. The top
 * level interface contains an array of master & slave interfaces. By default,
 * 16 master and 16 slave interfaces are defined in the top level interface.
 * Currently, the maximum master and slave interfaces supported is 450. The
 * number of master and slave interfaces in top level interface can be
 * controlled using macros SVT_AXI_MAX_NUM_MASTERS_{0..450} and
 * SVT_AXI_MAX_NUM_SLAVES_{0..450} respectively. For example, if you want to
 * use 8 master interfaces and 10 slave interfaces, you can define following
 * macros when compiling the VIP:
 * <li> SVT_AXI_MAX_NUM_MASTERS_8<br>
 * <li> SVT_AXI_MAX_NUM_SLAVES_10<br>
 * Please refer to User Guide section Overriding System Constants on how to
 * provide these macro definitions to VIP.
 *
 * <b>Sub-interfaces:</b><br>
 *
 * <table border="1" cellpadding="1" cellspacing="1"">
 *   <thead>
 *    <tr><th bgcolor="#dddddd"><b>Interface Name </b></th><th bgcolor="#dddddd"> Interface Description </th></tr>
 *   </thead>
 *   <tbody>
 *    <tr><td><a class="ClassLink" href="##intf_svt_axi_master_if"><b> svt_axi_master_if </b></td><td> Master Interface used by master VIP component </b></td><tr>
 *    <tr><td><a class="ClassLink" href="##intf_svt_axi_slave_if"><b> svt_axi_slave_if </b></td><td> Slave Interface used by slave VIP component </b></td><tr>
 *   </tbody>
 *  </table>
 *
 * <b>Clock signal description:</b><br>
 *
 * <table border="1" cellpadding="1" cellspacing="1"">
 *   <thead>
 *    <tr><th bgcolor="#dddddd"><b>Signal Name </b></th><th bgcolor="#dddddd"> Signal Description </th></tr>
 *   </thead>
 *   <tbody>
 *    <tr><td><b> common_aclk    </b></td><td> If all AXI interfaces in the system are expected to run on a common clock, user can use the common clock mode. Common clock mode can be selected by setting configuration parameter svt_axi_system_configuration::common_clock_mode to 1. This is the default mode of operation. When common clock mode is used, connect common_aclk signal to the top level clock signal in the testbench. Pleae note that if you are using the Interconnect VIP component, you are required to always connect common_aclk signalto a clock in the testbench, as Interconnect VIP component internally uses this clock for its operation.</b></td><tr>
 *    <tr><td><b> aclk </b></td><td> This signal belongs to the master and slave sub-interfaces. If all AXI interfaces in the system are expected to run on a different clock, user can use the aclk signal in the port interfaces. Set the configuration parameter svt_axi_system_configuration::common_clock_mode to 0. In this case, connect the aclk signal in each port interface to the appropriate port specific clock in the testbench.</b></td><tr>
 *   </tbody>
 *  </table>
 * 
 * <b>Clock connection examples:</b><br>
 * <li> Usage with common clock:<br>
 * assign axi_if.common_aclk = SystemClock;<br>
 *
 * <li> Usage with port specific clock:<br> 
 * assign axi_if.master_if[0].aclk = master_clk;<br>
 * assign axi_if.slave_if[0].aclk = slave_clk;<br>
 *
 * <b>Interface signal connections:</b><br>
 * <li>If master VIP is active, then connect DUT slave port signals to master interface:<br>
 * assign axi_if.master_if[0].awvalid =  DUT_slave_intf.awvalid;<br>
 *
 * <li>If master VIP is passive, then connect DUT master  port signals to master interface:<br>
 * assign axi_if.master_if[0].awvalid =  DUT_master_intf.awvalid;<br>
 *
 * <li>If slave VIP is active, then connect DUT master port signals to slave interface:<br>
 * assign axi_if.slave_if[0].awready =  DUT_master_intf.awready;    
 *
 * <li>If slave VIP is passive, then connect DUT slave port signals to slave interface:<br>
 * assign axi_if.slave_if[0].awready =  DUT_slave_intf.awready;
 *
 */

`ifdef SVT_AXI_SVC_SINGLE_INTERFACE
typedef virtual svt_axi_port_if AXI_MASTER_IF;
typedef virtual svt_axi_port_if AXI_SLAVE_IF;
`else
typedef virtual svt_axi_master_if AXI_MASTER_IF;
typedef virtual svt_axi_slave_if AXI_SLAVE_IF;
typedef virtual svt_axi_lp_if AXI_LP_IF;
`endif

interface svt_axi_if ();

  logic common_aclk;
/**
  * common active-low reset signal used specifically for the interconnect VIP component.
  * User needs to connect system reset to this interconnect reset signal
  * along with reset signals of other masters.
  * NOTE: This signal is strictly meant for internal use and mainly used for backward
  *       compatibility. User must not drive this signal from outside of this interface.
  */
  logic interconnect_resetn;

/** @cond PRIVATE */
  bit set_master_common_clk_flag = 0;
  bit set_slave_common_clk_flag = 0;
/** @endcond */

`ifndef SVT_AMBA_INTERFACE_METHOD_DISABLE
  // This assignment is strictly meant for internal use and done for backward compatibility
  assign interconnect_resetn = master_if[0].aresetn;
`endif

`ifdef CCI400_CHECKS_ENABLED
  typedef virtual svt_axi_cci400_config_if AXI_CCI400_CFG_IF;
  svt_axi_cci400_config_if cci400_config_if(common_aclk);
`endif

`ifndef SVT_AXI_MAX_NUM_MASTERS_0

`ifdef SVT_AXI_SVC_SINGLE_INTERFACE
  svt_axi_port_if master_if[`SVT_AXI_MAX_NUM_MASTERS-1:0](common_aclk);
`else
  svt_axi_master_if master_if[`SVT_AXI_MAX_NUM_MASTERS-1:0](common_aclk);
  svt_axi_lp_if lp_if[`SVT_AXI_MAX_LP_MASTERS-1:0]();
`endif
`endif

`ifndef SVT_AXI_MAX_NUM_SLAVES_0
`ifdef SVT_AXI_SVC_SINGLE_INTERFACE
  svt_axi_port_if  slave_if[`SVT_AXI_MAX_NUM_SLAVES-1:0](common_aclk);
`else
  svt_axi_slave_if  slave_if[`SVT_AXI_MAX_NUM_SLAVES-1:0](common_aclk);
`endif
`endif
  
/** @cond PRIVATE */
`ifdef CCI400_CHECKS_ENABLED
  function AXI_CCI400_CFG_IF get_cci400_config_if();
    get_cci400_config_if = cci400_config_if;
  endfunction
`endif
/** @endcond */


