    `define SVT_CHI_DONOTGOTOSD_WIDTH 1
    `define SVT_CHI_RETTOSRC_WIDTH 1
  `endif
 
  `ifdef SVT_CHI_ISSUE_B_ENABLE
    `define SVT_CHI_REQ_FLIT_LSB_QOS 0
    `define SVT_CHI_REQ_FLIT_LSB_TGTID (`SVT_CHI_QOS_WIDTH)
    `define SVT_CHI_REQ_FLIT_LSB_SRCID (`SVT_CHI_REQ_FLIT_LSB_TGTID+`SVT_CHI_TGT_ID_WIDTH)
    `define SVT_CHI_REQ_FLIT_LSB_TXNID (`SVT_CHI_REQ_FLIT_LSB_SRCID+`SVT_CHI_SRC_ID_WIDTH)
    `define SVT_CHI_REQ_FLIT_LSB_RETURNNID_STASHNID (`SVT_CHI_REQ_FLIT_LSB_TXNID+`SVT_CHI_TXN_ID_WIDTH)
