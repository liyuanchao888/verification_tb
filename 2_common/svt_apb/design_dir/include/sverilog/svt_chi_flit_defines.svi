    `define SVT_CHI_SNP_FLIT_LSB_STASHLPID_VALID (`SVT_CHI_SNP_FLIT_LSB_FWDNID+`SVT_CHI_FORWARD_NID_WIDTH+`SVT_CHI_STASH_LPID_WIDTH)
    `define SVT_CHI_SNP_FLIT_LSB_OPCODE          (`SVT_CHI_SNP_FLIT_LSB_FWDTXNID+`SVT_CHI_FORWARD_TXN_ID_WIDTH)
    `define SVT_CHI_SNP_FLIT_LSB_ADDR            (`SVT_CHI_SNP_FLIT_LSB_OPCODE+`SVT_CHI_SNP_VC_FLIT_OP_WIDTH)
    `define SVT_CHI_SNP_FLIT_LSB_NS              (`SVT_CHI_SNP_FLIT_LSB_ADDR+`SVT_CHI_SNP_ADDR_WIDTH)
    `define SVT_CHI_SNP_FLIT_LSB_DONOTGOTOSD     (`SVT_CHI_SNP_FLIT_LSB_NS+`SVT_CHI_NS_WIDTH)
    `define SVT_CHI_SNP_FLIT_LSB_RETTOSRC        (`SVT_CHI_SNP_FLIT_LSB_DONOTGOTOSD+`SVT_CHI_DONOTGOTOSD_WIDTH)
    `define SVT_CHI_SNP_FLIT_LSB_TRACETAG        (`SVT_CHI_SNP_FLIT_LSB_RETTOSRC+`SVT_CHI_RETTOSRC_WIDTH)
    `ifdef SVT_CHI_ISSUE_D_ENABLE
      `define SVT_CHI_SNP_FLIT_LSB_MPAM          (`SVT_CHI_SNP_FLIT_LSB_TRACETAG+`SVT_CHI_TRACE_TAG_WIDTH)
    `endif
