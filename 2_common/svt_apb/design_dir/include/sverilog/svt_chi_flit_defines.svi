  //CHI flit width for pack and unpack functions. Max possible width the spec permits for each channels. 
  `ifdef SVT_CHI_ISSUE_E_ENABLE
    `define SVT_CHI_REQ_PACK_UNPACK_WIDTH 194
    `define SVT_CHI_RSP_PACK_UNPACK_WIDTH 73
    `define SVT_CHI_SNP_PACK_UNPACK_WIDTH 119
  `elsif SVT_CHI_ISSUE_D_ENABLE
    `define SVT_CHI_REQ_PACK_UNPACK_WIDTH 184
    `define SVT_CHI_RSP_PACK_UNPACK_WIDTH 66
    `define SVT_CHI_SNP_PACK_UNPACK_WIDTH 115
  `elsif SVT_CHI_ISSUE_B_ENABLE
    `define SVT_CHI_REQ_PACK_UNPACK_WIDTH 169
    `define SVT_CHI_RSP_PACK_UNPACK_WIDTH 59
    `define SVT_CHI_SNP_PACK_UNPACK_WIDTH 100
  `else  
    `define SVT_CHI_REQ_PACK_UNPACK_WIDTH 128
    `define SVT_CHI_RSP_PACK_UNPACK_WIDTH 45
    `define SVT_CHI_SNP_PACK_UNPACK_WIDTH 65
  `endif  

`endif


