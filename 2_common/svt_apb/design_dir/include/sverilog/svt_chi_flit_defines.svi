    `define SVT_CHI_DAT_FLIT_LSB_TAGOP      (`SVT_CHI_DAT_FLIT_LSB_DATAID+`SVT_CHI_DATA_ID_WIDTH)
    `define SVT_CHI_DAT_FLIT_LSB_TAG        (`SVT_CHI_DAT_FLIT_LSB_TAGOP+`SVT_CHI_TAGOP_WIDTH)
    `define SVT_CHI_DAT_FLIT_LSB_TAG_UPDATE (`SVT_CHI_DAT_FLIT_LSB_TAG+`SVT_CHI_DAT_FLIT_MAX_TAG_WIDTH)
    `define SVT_CHI_DAT_FLIT_LSB_TRACETAG   (`SVT_CHI_DAT_FLIT_LSB_TAG_UPDATE+`SVT_CHI_DAT_FLIT_MAX_TAG_UPDATE_WIDTH)
  `else
    `define SVT_CHI_DAT_FLIT_LSB_TRACETAG   (`SVT_CHI_DAT_FLIT_LSB_DATAID+`SVT_CHI_DATA_ID_WIDTH)
  `endif
    `define SVT_CHI_DAT_FLIT_LSB_RSVDC      (`SVT_CHI_DAT_FLIT_LSB_TRACETAG+`SVT_CHI_TRACE_TAG_WIDTH)
    `define SVT_CHI_DAT_FLIT_LSB_BE         (`SVT_CHI_DAT_FLIT_LSB_RSVDC+`SVT_CHI_DAT_FLIT_MAX_RSVDC_WIDTH)
    `define SVT_CHI_DAT_FLIT_LSB_DATA       (`SVT_CHI_DAT_FLIT_LSB_BE+`SVT_CHI_DAT_FLIT_MAX_BE_WIDTH)
    `define SVT_CHI_DAT_FLIT_LSB_DATACHECK  (`SVT_CHI_DAT_FLIT_LSB_DATA+`SVT_CHI_DAT_FLIT_MAX_DATA_WIDTH)
    `define SVT_CHI_DAT_FLIT_LSB_POISON     (`SVT_CHI_DAT_FLIT_LSB_DATACHECK+(`SVT_CHI_DAT_FLIT_MAX_DATACHECK_WIDTH*`SVT_CHI_DATACHECK_INTERNAL_WIDTH_ENABLE))

    // SNP flit
    `define SVT_CHI_SNP_FLIT_LSB_QOS             0
    `define SVT_CHI_SNP_FLIT_LSB_SRCID           (`SVT_CHI_QOS_WIDTH)
    `define SVT_CHI_SNP_FLIT_LSB_TXNID           (`SVT_CHI_SNP_FLIT_LSB_SRCID+`SVT_CHI_SRC_ID_WIDTH)
    `define SVT_CHI_SNP_FLIT_LSB_FWDNID          (`SVT_CHI_SNP_FLIT_LSB_TXNID+`SVT_CHI_TXN_ID_WIDTH)
